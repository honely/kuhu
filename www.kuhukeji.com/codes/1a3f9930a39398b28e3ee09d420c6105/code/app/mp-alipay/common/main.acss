}
}
@-webkit-keyframes action_skew{
0%{-webkit-transform: skew(-40deg);transform: skew(-40deg);opacity: 0;}
50%{ -webkit-transform: skew(40deg); transform: skew(40deg);opacity: 0.2;}
100%{ -webkit-transform: skew(0deg); transform: skew(0deg);opacity: 1;}
}
@keyframes action_skew{
0%{-webkit-transform: skew(-40deg);transform: skew(-40deg);opacity: 0;}
50%{ -webkit-transform: skew(40deg); transform: skew(40deg);opacity: 0.2;}
100%{ -webkit-transform: skew(0deg); transform: skew(0deg);opacity: 1;}
}
@-webkit-keyframes action_rotateY{
0%{-webkit-transform: rotateY(0deg);transform: rotateY(0deg);opacity: 0;}
50%{ -webkit-transform: rotateY(180deg); transform: rotateY(180deg);opacity: 0.2;}
100%{ -webkit-transform: rotateY(360deg); transform: rotateY(360deg);opacity: 1;}
}
@keyframes action_rotateY{
0%{-webkit-transform: rotateY(0deg);transform: rotateY(0deg);opacity: 0;}
50%{ -webkit-transform: rotateY(180deg); transform: rotateY(180deg);opacity: 0.2;}
100%{ -webkit-transform: rotateY(360deg); transform: rotateY(360deg);opacity: 1;}
}
@-webkit-keyframes action_translateY{
0%{-webkit-transform: translateY(50px);transform: translateY(50px);opacity: 0;}
100%{ -webkit-transform: translateY(0px); transform: translateY(0px);opacity: 1;}
}
@keyframes action_translateY{
0%{-webkit-transform: translateY(50px);transform: translateY(50px);opacity: 0;}
100%{ -webkit-transform: translateY(0px); transform: translateY(0px);opacity: 1;}
}
@-webkit-keyframes action_scale{
0%{-webkit-transform: scale(0.2);transform: scale(0.2);opacity: 0;}
100%{ -webkit-transform: scale(1); transform: scale(1);opacity: 1;}
}
@keyframes action_scale{
0%{-webkit-transform: scale(0.2);transform: scale(0.2);opacity: 0;}
100%{ -webkit-transform: scale(1); transform: scale(1);opacity: 1;}
}
/*---淡出----*/
@-webkit-keyframes action_skewOut{
0%{-webkit-transform: skew(40deg);transform: skew(40deg);opacity: 1;}
50%{ -webkit-transform: skew(-40deg); transform: skew(-40deg);opacity: 0.2;}
100%{ -webkit-transform: skew(0deg); transform: skew(0deg);opacity: 0;}
}
@keyframes action_skewOut{
0%{-webkit-transform: skew(40deg);transform: skew(40deg);opacity: 1;}
50%{ -webkit-transform: skew(-40deg); transform: skew(-40deg);opacity: 0.2;}
100%{ -webkit-transform: skew(0deg); transform: skew(0deg);opacity: 0;}
}
@-webkit-keyframes action_rotateYOut{
0%{-webkit-transform: rotateY(360deg);transform: rotateY(360deg);opacity: 1;}
50%{ -webkit-transform: rotateY(180deg); transform: rotateY(180deg);opacity: 0.2;}
100%{ -webkit-transform: rotateY(0deg); transform: rotateY(0deg);opacity: 0;}
}
@keyframes action_rotateYOut{
0%{-webkit-transform: rotateY(360deg);transform: rotateY(360deg);opacity: 1;}
50%{ -webkit-transform: rotateY(180deg); transform: rotateY(180deg);opacity: 0.2;}
100%{ -webkit-transform: rotateY(0deg); transform: rotateY(0deg);opacity: 0;}
}
@-webkit-keyframes action_translateYOut{
0%{-webkit-transform: translateY(0px);transform: translateY(0px);opacity: 1;}
100%{ -webkit-transform: translateY(50px); transform: translateY(50px);opacity: 0;}
}
@keyframes action_translateYOut{
0%{-webkit-transform: translateY(0px);transform: translateY(0px);opacity: 1;}
100%{ -webkit-transform: translateY(50px); transform: translateY(50px);opacity: 0;}
}
@-webkit-keyframes action_scaleOut{
0%{-webkit-transform: scale(1);transform: scale(1);opacity: 1;}
100%{ -webkit-transform: scale(0.2); transform: scale(0.2);opacity: 0;}
}
@keyframes action_scaleOut{
0%{-webkit-transform: scale(1);transform: scale(1);opacity: 1;}
100%{ -webkit-transform: scale(0.2); transform: scale(0.2);opacity: 0;}
}
