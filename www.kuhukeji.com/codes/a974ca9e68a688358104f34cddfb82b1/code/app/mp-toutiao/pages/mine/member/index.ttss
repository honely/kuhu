}
.member-box .member-menu {
	width: 100%;
	height: 148px;
	padding: 24px 0;
	background: #fff;
}
.member-box .member-menu .dot-box {
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
