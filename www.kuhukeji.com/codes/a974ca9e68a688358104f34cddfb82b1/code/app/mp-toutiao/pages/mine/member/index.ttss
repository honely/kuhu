}
.member-box .member-menu .swiper-box {
	width: 100%;
	height: 94px;
}
.swiper-box .item-menu {
	width: 25%;
	height: 94px;
	text-align: center;
	float: left;
}
.swiper-box .item-menu .m-icon {
	width: 50px;
	height: 50px;
	border-radius: 25px;
	/* background: #f00; */
}
.swiper-box .item-menu .m-name {
	color: #A65F34;
	font-size: 14px;
	margin-top: 12px;
}

