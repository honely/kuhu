
.dialog-payment-box {
	width: 100%;
	min-height: 460px;
	background: #F2F4F7;
	position: fixed;
	bottom: 0;
	display: block;
	animation: action_translateY 1s ease 0s 1 both;
	-webkit-animation: action_translateY 1s ease 0s 1 both;
	border-radius: 8px 8px 0 0;
}
.dialog-payment-box .db-header {
	width: 100%;
	height: 54px;
	line-height: 54px;
	border-radius: 8px 8px 0 0;
	background: #fff;
	padding: 0 15px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.db-header .h-lt {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	color: #000;
	font-size: 18px;
}
.db-header .h-rt {
	font-size: 16px;
	color: #333333;
}
.dialog-payment-box .db-cont {
	width: 100%;
	padding: 8px 15px;
	display: block;
}
.dialog-payment-box .db-cont .item-options {
	width: 100%;
	height: 60px;
	line-height: 60px;
	border-radius: 8px;
	background: #fff;
	display: -webkit-inline-box;
	display: -webkit-inline-flex;
	display: -ms-inline-flexbox;
	display: inline-flex;
	margin: 6px 0;
	padding: 0 15px;
}
.item-options .io-lt {
	width: 100%;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	color: #333333;
	font-size: 16px;
}
.item-options .io-rt {
	font-size: 32px;
}
.manjian-box,
.discount-box,
.coupon-box {
	width: 100%;
	display: block;
	background: #fff;
	padding: 16px 0;
}
.manjian-box .mj-tit,
.discount-box .db-tit,
.coupon-box .cb-tit {
	width: 100%;
	height: 30px;
	line-height: 30px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	padding: 0 15px;
}
.t-lt {
	width: 100%;
	height: 30px;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.t-lt text {
	font-size: 18px;
	color: #000;
	border-bottom: 4px solid #DCB65C;
}
.t-rt {
	color: #666666;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.manjian-box .mj-cont,
.discount-box .db-cont,
.coupon-box .cb-cont {
	width: 96%;
	display: -webkit-inline-box;
	display: -webkit-inline-flex;
	display: -ms-inline-flexbox;
	display: inline-flex;
	/* overflow-y: scroll; */
	margin: 0 2%;
}
.cb-cont .header {
	width: 100%;
	height: 60px;
	line-height: 60px;
	border-bottom: 1px solid #F2F4F5;
}
.cb-cont .detail-cont {
	width: 100%;
	padding: 20px 0;
	border-bottom: 1px solid #F2F4F5;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.cb-cont .detail-cont .item-upgrade {
	width: 100%;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	display: block;
	text-align: center;
	color: #000000;
	font-size: 14px;
}
.item-upgrade .ig-tit {
	color: #000000;
	font-size: 14px;
}
.item-upgrade .ig-cont {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	margin-top: 8px;
}
.ig-cont .icon-coin {
	width: 22px;
	height: 22px;
	margin-right: 2px;
}
.cb-cont .detail-cont .item-upgrade .num {
	font-size: 20px;
	color: #ED3005;
	margin-left: 8px;
}
.cb-cont .detail-cont .upgrade-arrow {
	width: 32px;
	height: 36px;
}

/* 通知轮播 */
.cb-cont .notice-box {
	width: 100%;
	height: 48px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.cb-cont .notice-box .icon-notice {
	width: 16px;
	height: 14px;
	margin: 17px 0;
}
.cb-cont .notice-box .notice-list {
	width: 100%;
	padding: 11px 8px;
	font-size: 12px;
}

/* uni-swiper .uni-swiper-dot-active{
	background: #fff!important;
} */
.notice-list .item-notice {
	width: 100%;
	height: 25px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	font-size: 14px;
	color: #FFAE19;
}
.notice-list .item-notice .phone {
	color: #2D3133;
	margin: 0 8px;
	font-size: 14px;
}
.scroll-box {
	display: -webkit-inline-box;
	display: -webkit-inline-flex;
	display: -ms-inline-flexbox;
	display: inline-flex;
}
.scroll-box .scroll-module {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	white-space: nowrap;
}
.scroll-box .scroll-module .item-goods {
	width: 48%;
	margin: 16px 8px;
	display: inline-block;
}
.scroll-box .scroll-module .item-goods .g-img {
	width: 100%;
	height: 165px;
	position: relative;
	background: #f00;
}
.item-goods .g-img .img {
	width: 100%;
	height: 100%;
	background: #fcfcfc;
}
.item-goods .g-img .tag {
	height: 20px;
	line-height: 20px;
	padding: 0 6px;
	border-radius: 0px 0px 10px 0px;
	background: #FF8C00;
	color: #fff;
	font-size: 12px;
	position: absolute;
	top: 0;
	left: 0;
}
.item-goods .g-name {
	width: 100%;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	margin: 12px 0;
	color: #333333;
	font-size: 16px;
}
.item-goods .g-price {
	width: 100%;
	height: 22px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: end;
	-webkit-align-items: flex-end;
	    -ms-flex-align: end;
	        align-items: flex-end;
}
.item-goods .g-price .shop-price {
	color: #ED3005;
	margin-right: 8px;
}
.shop-price .mark {
	font-size: 14px;
}
.shop-price .value {
	font-size: 18px;
}
.item-goods .g-price .market-price {
	color: #999999;
	font-size: 14px;
}
.market-price .value {
	text-decoration: line-through;
}

/* 折扣商品 */
.scroll-box .scroll-module .item-discount {
	width: 48%;
	margin: 16px 8px;
	display: inline-block;
}
.scroll-box .scroll-module .item-discount .d-img {
	width: 100%;
	height: 165px;
	position: relative;
	background: #f00;
}
.item-discount .d-img .img {
	width: 100%;
	height: 100%;
	background: #fcfcfc;
}
.item-discount .d-img .tag {
	height: 20px;
	line-height: 20px;
	padding: 0 10px;
	border-radius: 0px 0px 8px 0px;
	background: -webkit-gradient(linear, left top, right top, from(rgba(255, 164, 75, 1)), to(rgba(251, 74, 134, 1)));
	background: -o-linear-gradient(left, rgba(255, 164, 75, 1) 0%, rgba(251, 74, 134, 1) 100%);
	background: linear-gradient(90deg, rgba(255, 164, 75, 1) 0%, rgba(251, 74, 134, 1) 100%);
	color: #fff;
	font-size: 12px;
	position: absolute;
	top: 0;
	left: 0;
}
.item-discount .d-name {
	width: 100%;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	margin: 12px 0;
	color: #333333;
	font-size: 16px;
}
.item-discount .m-price {
	color: #ED3005;
	font-size: 14px;
}
.item-discount .d-price {
	color: #999999;
	font-size: 14px;
	margin-top: 6px;
}

/* 底部按钮 */
.bottom-box {
	width: 100%;
	height: 60px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-shadow: 0px 0px 4px 0px rgba(0, 0, 0, 0.04);
	        box-shadow: 0px 0px 4px 0px rgba(0, 0, 0, 0.04);
	position: fixed;
	bottom: 0;
}
.bottom-box .item-bottom {
	width: 50%;
	height: 60px;
	background: -webkit-gradient(linear, left top, right top, from(rgba(255, 164, 75, 1)), to(rgba(251, 74, 134, 1)));
	background: -o-linear-gradient(left, rgba(255, 164, 75, 1) 0%, rgba(251, 74, 134, 1) 100%);
	background: linear-gradient(90deg, rgba(255, 164, 75, 1) 0%, rgba(251, 74, 134, 1) 100%);
	padding: 0 15px;
}
.member-box {
	width: 100%;
	display: block;
}
.member-box .member-base {
	width: 100%;
	height: 176px;
	padding-top: 16px;
	position: relative;
}
.member-box .member-base .img-bg {
	width: 100%;
	height: 160px;
}
.member-box .member-base .m-grade-desc {
	width: 100%;
	height: 30px;
	line-height: 30px;
	padding: 0 30px;
	position: absolute;
	top: 26px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
