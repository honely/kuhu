/*每个页面公共css */
.pageContainer {
	width: 100%;
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	overflow: scroll;
}
.more, .nodata {
	width: 100%;
	height: 50px;
	line-height: 50px;
	border-bottom: 1px #f5f5f5 solid;
	text-align: center;
	font-size: 14px;
	color: #22c37d;
}
.nodata {
	color: #999;
}
::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}
.main-box{
	width: 100%;
	display: block;
}
.main-box .tab-box {
	width: 100%;
	background: #fff !important;
	display: -webkit-inline-box;
	display: -webkit-inline-flex;
	display: -ms-inline-flexbox;
	display: inline-flex;
	position: fixed;
	z-index: 998;
}
.ellipsis-01, .ellipsis-02, .ellipsis-03 {
	word-break: break-all;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	overflow: hidden;
}
.ellipsis-01{
	-webkit-line-clamp: 1;
}
.ellipsis-02{
	-webkit-line-clamp: 2;
}
.ellipsis-03{
	-webkit-line-clamp: 3;
}
.mask{		
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background: rgba(0, 0, 0, 0.5);
	overflow: hidden;
	z-index: 999;
	color: #fff;
}
.dialog-common{
	width: 100%;
	min-height: 700rpx;
	max-height: 1100rpx;
	background: #fff;
	border-radius: 30rpx 30rpx 0 0;
	position: fixed;
	bottom: 0;
	display: block;
	animation: bounceInUp 1s ease 0s 1 both;
	-webkit-animation: bounceInUp 1s ease 0s 1 both;
}
.dialog-common .dc-header{
	width: 100%;
	height: 98rpx;
	padding: 0 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	border-bottom: 1rpx #e6e6e6 solid;
}
.dc-header .h-title{
	width: 100%;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
